<!DOCTYPE html>
<html>
  <head>
    <title>Message Board</title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1>Message Board</h1>
    <p>Welcome to the board</p>
    <% messages.forEach(function(message) { %>
    <h3><%= message.username%></h3>
    <p><%= message.text%></p>
    <p><%= message.added.toLocaleDateString() %></p>
    <% }) %>
    <button type="button"><a href="/new">New Message</a></button>
  </body>
</html>
